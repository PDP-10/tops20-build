	FFFVER==100,,4		;VERSION NUMBER

DEFINE $F1,<>
DEFINE $F2,<>

	LOC 137			; 1.4  add a version number
	FFFVER			; 1.4

$LIBDEF	(FFF, FF, FFFDYN, FFFVER, VM%MIG+VM%MAG, "DYNLIB$FFF%", "SYS:FFF.EXE",<FFFDYI>,<<F$OPEN,,,$F1>	,
