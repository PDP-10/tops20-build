LZ:	HRLZ	1,ESCCOD		;ASSIGN TO CHAN. 0
	ATI
	CAIL	T,"A"+40		;LOWER CASE A
	 CAILE	T,"Z"+40		;LOWER CASE Z
	  CAIA			;NOT LOWERCASE
