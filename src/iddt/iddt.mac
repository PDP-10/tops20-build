;ENTRY VECTOR
EVEC:	JRST	INIT		;"START"
	JRST	INIT		;"REENETER"
	JRST	SPLICD		;INFERIOR HAS BEEN SPLICED BY EXEC
VERSN:	^D9			;#01 We are now version 9
PATVER:	1			;#02                     .1


;FORK HANDLE SUPPLIED IN AC1 FROM EXEC
