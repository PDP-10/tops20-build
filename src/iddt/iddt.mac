	TRNN	F,SEMIF		;SEMICOLON CONTROL, OR...
	 TLNE	F,CF		;ALT. MODE CONTROL FLAG?
	  JRST	L21		;CONTROL
	CAIG	T,"Z"		;Z
	 CAIGE	T,"A"		;A
	  JRST	L21		;NOT A LETTER
	JRST	LET


;CONTROL OR NOT-LETTER
L21:	CAILE	T,137
	 JRST	ERR
	TRNE	F,SEMIF
	 ADDI	T,140		;USE OTHER TABLE
	MOVE	R,T
	IDIVI	R,3		;REMAINDER GIVES COLUMN, QUOTIENT GIVES ROW
