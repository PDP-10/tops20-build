	AOS	2
	PMAP
	SOJN	4,.-2
	MOVE	0,[LOW,,HOME]
	BLT	0,DDTEND-1
	HRLZ	0,116
	HRRI	0,DDTEND
	HLRE	1,116
	MOVEI	2,DDTEND-1
	SUB	2,1
	BLT	0,0(2)

	MOVEI	0,DDTEND
