	SETOM	PINFF	;PRINT APPROPRIATE AC AND EFF ADR
	CALL	LI1	;EXAMINE THE BREAK LOCATION, SETUP "."
	CALL	LCT
	HRRZ	W,WHY
	CAIE	W,10	;INTERRUPT CODE
	 TRNN	F,XEQ!CONDX
	JRST	DD2
