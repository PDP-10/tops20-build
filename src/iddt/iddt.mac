	 JRST [	CAIN W,DIVD-DDT	;#01 and its a '
		 JRST DDT(W)	;#01 NO EVAL
		JRST .+1 ]	;#01 Otherwise proceed as before

	MOVE	T,SYL
	TLZN	F,LTF
	 JRST	POWER

	CAIN	W,SPACE-DDT	;IS TERMINATOR A SPACE?
	 SKIPE	WRD		;AND WORD SO FAR ZERO?
