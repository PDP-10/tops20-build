	MOVEM CX,PFHD1		;SAVE IT
	MOVE CX,KCPFD2		;GET PAGE FAIL DATA 2 WORD
	MOVEM CX,PFHD2		;SAVE IT
	MOVE CX,KCPFOF		;GET TRAP FLAGS
	MOVEM CX,PFHFL		;SAVE ON THE STACK
   >				;END OF IFN KCFLG
