	  CAIGE 4,NOFN 		;DEST IS FORK, SOURCE ALSO?
	  RETBAD(PMAPX4)		;NO, FILE-TO-FORK ILLEGAL
	ENDIF.
	HRRZ T3,T2
	MOVE 3,CXBPGA(3)	;GET PTR
	JUMPE 3,MVPT3		;NOP IF NULL
	LOAD 4,PTRCOD,3		;GET PTR TYPE
	CAIN 4,IMMCOD		;PRIVATE AND IN CORE?
	TLNE 3,(NCORTM)
