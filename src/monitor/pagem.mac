;Set write bit in the CST

NWRBS0:	HRRZS T1,TRPPTR		;GET THE PHYSICAL PAGE NUMBER
	SETONE CSWRB,+T1	;SET THE BIT
	LOAD T1,STGADR,TRPPTR	;GET ADDRESS
