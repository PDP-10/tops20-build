P%SHUN==44

; SET UNIT CHARACTERISTICS COMMAND MESSAGE
P.FORM==.MHUDA+<40>/4		;FORMAT (2 BYTES)
P.CPSP==.MHUDA+<42>/4		;*COPY SPEED (2 BYTES)
