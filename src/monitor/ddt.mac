;STILL IFE FTFILE

;USER UUO HANDLER

USRUUO:	MOVE	S,PCWRD		;PC OF USER INSTRUCTION
	TLNE	S,-1		;IN SECTION 0?
	JRST	XUSRUU		;[521] NO, HANDLED DIFFERENTLY
	MOVEI	R,.JBUUO	;SETUP JOBUUO
	EXCH	F,FLAGS		;RESTORE REGULAR FLAGS
	MOVE	T,I.NST		;FETCH INST WITH EFF ADR @ I.NSTE
	TXZ	T,@		;CLEAR @ BIT
	HRR	T,I.NSTE	;MERGE IN COMPUTED EFFECTIVE ADDR
	PUSHJ	P,DEPMEM	;STORE USER UUO IN .JBUUO IF POSSIBLE
	 POPJ	P,		;MEMORY WRITE ERROR (?)
	EXCH	F,FLAGS		;RESTORE $X FLAGS
	MOVE	T,[XCT 41]	;PRETEND INSTRUCTION WAS AN XCT
	JRST	$X03

;HERE TO HANDLE AN LUUO IN A NON-ZERO SECTION

XUSRUU:	PUSHJ	P,RDLUUB	;[521] READ ADDRESS OF LUUO BLOCK
	SKIPN	R,T		;[521] IS THERE ONE?
	JRST	SETEA		;[521] NO, JUST TRUST TO LUCK
	MOVE	T,PCFLG		;[521] BUILD LUUO INFO
	HRR	T,F		;[521] PC FLAGS,,OPCODE
	DPB	W1,[POINT 4,T,30]  ;[521] AND AC
	EXCH	F,FLAGS		;[521] SWITCH TO NORMAL DDT FLAGS
	PUSHJ	P,DEPMEM	;[521] FILL IN FIRST WORD IN LUUO BLOCK
	 POPJ	P,		;[521] WRITE ERROR?
	AOS	R		;[521][645] POINT TO SECOND WORD
	MOVE	T,I.NPC		;[521] SECOND WORD GETS LUUO'S PC
	HRRI	T,1(T)		;[521]  PLUS ONE (I.E. LUUO RETURN ADDR)
	PUSHJ	P,DEPMEM	;[521] FILL THAT IN
	 POPJ	P,		;[521] CAN'T?
	AOS	R		;[521][645] POINT TO THIRD WORD
	MOVE	T,I.NSTE	;[521] 3RD WORD GETS E OF LUUO
	PUSHJ	P,DEPMEM	;[521] STUFF IT
	 POPJ	P,		;[521] ??
	AOS	R		;[521][645] POINT TO FOURTH WORD
	MOVEM	R,I.NSTE	;[521] PRETEND THE LUUO WAS REALLY
	MOVX	T,XJRST		;[521]  AN XJRST THROUGH THE FOURTH WORD
	MOVEM	T,I.NST		;[521] SET PROTO INSTRUCTION
	EXCH	F,FLAGS		;[521] BACK TO $X FLAGS
	JRST	IJ$15		;[521] TREAT LIKE XJRST NOW

  IFN FTDEC10,<
RDLUUB:	MOVX	T,<.UTRED,,TT1>	;[635] UUO ARG <READ,,BLOCK>
	MOVEI	TT1,2		;[635] TWO WORDS FOLLOWING
	MOVX	TT2,.UTLUU	;[635] TRAP TYPE IS LUUO BLOCK
	UTRP.	T,		;[635] READ THE BLOCK ADDRESS
	  TDZA	T,T		;[635] NONE AFTER ALL?
	MOVE	T,TT3		;[635] RETURN IT IN CORRECT AC
	POPJ	P,		;[635] GOT IT
  > ;END IFN FTDEC10

  IFN FTDEC20,<
RDLUUB:	PUSHJ	P,SAVT4		;[521] PRESERVE JSYS ACS
	MOVX	T1,.FHSLF	;[521] THIS PROCESS'
	MOVX	T2,.SWRLT	;[521] LUUO BLOCK
	SWTRP%			;[521] READ ADDRESS
	MOVE	T,T3		;[521] RETURN IN A USEFUL AC
	POPJ	P,		;[521] ALL DONE
  > ;END IFN FTDEC20
;STILL IFE FTFILE

;INTERPRET FSC

IFSC:	TLO	F,FAC+FLA+IMM	;FLOATING AC, FIXED IMMEDIATE E
