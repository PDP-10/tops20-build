STORE9:	JSP	TT2,PZAFI	;[630] MAY WANT TO DO THIS IN SECTION 1
	MOVEM	T,@R		;DO THE STORE
	JRST	CPOPJ1		;SUCCESSFUL RETURN
  > ;END IFN FTDEC10
;FALL THROUGH FROM STORE 8

  IFN FTDEC20,<
	TXNN	TT,PG$WRT	;WRITEABLE?
	JRST [	SMERS	<NOT WRITABLE>
		POPJ	P,]
	MOVE	TT2,R
	PUSHJ	P,CRESEC	;[603] CHECK AND CREATE SECTION IF NECESSARY
	SKIPN	SAVSTS		;[570] WAS PSI ON?
	JRST	STORE9		;[570] NO, DON'T WORRY ABOUT PAGE-CREATE TRAPS
	DMOVEM	T1,TTEM1	;[570] SAVE SOME TEMPS
	MOVEI	T1,.FHSLF	;[570] FIND OUT OUR PROCESS'
	RCM%			;[570]  ACTIVE CHANNELS
	TXNN	T1,1B<.ICNXP>	;[570] PAGE CREATES BEING TRAPPED?
	JRST [	DMOVE	T1,TTEM1	;[570] NO, RESTORE THE ACS
		JRST	STORE9]		;[570]  AND KEEP GOING
	MOVEI	T1,.FHSLF	;[570] PAGE CREATES BEING TRAPPED, WE HAVE
	MOVX	T2,1B<.ICNXP>	;[570]  TO DEACTIVATE THE CHANNEL IN ORDER
	DIC%			;[570]  TO PREVENT A LATER INTERRUPT
	SETOM	SAVNXP		;[570] REMEMBER WE DID THIS
	DMOVE	T1,TTEM1	;[570] RESTORE ACS
	CAIA			;[570] AND FALL INTO STORE9
STORE9:	SETZM	SAVNXP		;[570] PAGE CREATES NOT BEING TRAPPED
	JSP	TT2,PCAFI	;SUPPRESS ADDRESS BREAK AND
	MOVEM	T,@R		;DO THE STORE
	ERJMP [	SMERS	<UNEXPECTED MOVEM FAILURE>
		SOS	(P)	;[570] FORCE LATER ERROR RETURN
		JRST	.+1]	;[570] CHECK PSI CHANNEL
	SETOM	LASTPG		;STORE COULD HAVE CHANGED PROPERTIES
	SKIPN	SAVNXP		;[570] DID WE DISABLE THE CHANNEL?
	JRST	CPOPJ1		;[570] NO, ALL DONE
	DMOVEM	T1,TTEM1	;[570] SAVE ACS AGAIN
	MOVEI	T1,.FHSLF	;[570] WE NEED TO RE-ACTIVATE THE
	MOVX	T2,1B<.ICNXP>	;[570]  PSI CHANNEL SO THE USER CAN'T
	AIC%			;[570]  TELL WE FOOLED WITH IT
	DMOVE	T1,TTEM1	;[570] GET ACS BACK
	JRST	CPOPJ1		;SUCCESSFUL RETURN

;CHECK FOR NONX SECTION AND CREATE IT

CRESEC:	HLRZ	TT,TT2		;[603] GET SECTION INTO TT
	PUSHJ	P,CHKSEC	;[603] SEE IF IT'S THERE
	TXNE	TT,PG$SXI	;[630][603] SECTION EXIST?
	POPJ	P,		;[603] YES, ALL DONE
	PUSHJ	P,SAVT4		;SAVE JSYS ARG ACS
	SETZ	T1,		;CREATE PRIV SECTION
	HLRZ	T2,TT2
	HRLI	T2,.FHSLF
	MOVX	T3,PM%RD+PM%WR+1 ;1 SECTION, READ AND WRITE
	SMAP%
	 ERJMP	CPOPJ		;FAILED, JUST CONTINUE
	POPJ	P,
  > ;END OF IFN FTDEC20
