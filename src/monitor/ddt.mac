	MAP	TT1,@TT		;[421] CHECK SECTION 0
	POP	P,TT		;[421] GET PREV. DATA
	CAME	TT1,TT		;[421] SAME?
	 POPJ	P,		;[421] NO, PAGES NOT EQUIVALENT
	JRST	CPOPJ1		;[421] YES, PAGES EQUIVALENT
