;MDDT CHECKPOINT DEFINITIONS

IFN FTMON,<
  DEFINE CHKIN (%CKLAB) <
	SKIPLE	CHKCNT			;;[612] DO WE ALREADY HAVE THE LOCK?
	JRST	%CKLAB			;;[612] YES
	AOSE	@[MSEC1,,MDDLCK]	;;[612] NO, ATTEMPT TO TAKE LOCK
	JRST [	TSTR	<?MDDT BREAKPOINT BLOCK ALREADY IN USE>
		POPJ	P,]		;;[612] FAIL
	PUSH	P,@[MSEC1,,FORKX]	;;[612] SUCCEED
	POP	P,@[MSEC1,,MDDFX]	;;[612] SHOW THE LOCK IS OURS.
%CKLAB:	AOS	CHKCNT>			;;[612] INCREMENT OUR USE COUNT.

  DEFINE CHKOUT<
	SKIPG	CHKCNT			;;[612] DO WE HAVE THE LOCK?
