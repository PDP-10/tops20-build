	PAGE.	T,		;MAP SECTION 0 INTO SECTION 1
	 JRST	SYMNZ8		;OH WELL, TIME TO GIVE UP
	MOVEI	W1,[ASCIZ\
[DDT - Section 0 mapped into section 1 for NZS symbol table access]
\]				;INFORMATIVE TEXT
	PUSHJ	P,TSTRG		;TO TELL THE USER WHAT HAPPENED
