PDVBLK:	MOVEM	R,SYTLOC	;SAVE PDVA
	SETZ	W1,		;CLEAR FLAGS
;	ADDI	R,.PVCNT	;GET PDV LENGTH
	PUSHJ	P,FETCH		;INTO T
	 JRST	PDVS90		;CAN'T -- DONE EARLY
