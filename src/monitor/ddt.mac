      $$XW.2==<<<<OP>_-^D27>-$XW$L> - $$XW.1*^D36>;;BIT WITHIN INDEX WORD
      $$XW1 (TBL,\$$XW.1,\$$XW.2)		;;SET THE BIT
    > ;;END IRP
  > ;;END OF $$X'TBL
> ;END OF $$XW

